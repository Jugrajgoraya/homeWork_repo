<%- include('partials/header.ejs')  %> 
    <div>
        <% for ( cohort of cohorts ) {%> 
        <div class="card mt-4 mb-4">
            <div class="card-body">
                <div class ="row">
                    <img class="rounded-circle" src="<%- cohort.logoUrl  %>" alt="logo" style ="height: 2.5rem; width:4.5rem;">
                    <h5 class="card-title col"><%- cohort.name  %></h5>
                </div>
                <p class="card-text"><%- cohort.members  %></p>
                <a href="/cohorts/<%- cohort.id %> " class="btn btn-primary">Show Cohort</a>
            </div>
        </div>
        <% } %> 
    </div>

<%- include('partials/footer.ejs')  %>     